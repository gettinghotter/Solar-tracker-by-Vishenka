#define PIN_PHOTO_SENSOR A0 
#define PIN_PHOTO_SENSOR A1 
#include <Servo.h>  
 
Servo servoHorisontal;
Servo servoVertical;

const int photoSensorA0 = A0;
const int photoSensorA1 = A1;
const int photoSensorA2 = A2;
const int photoSensorA3 = A3;

int angle; 
 
void setup() { 
  
  servoHorisontal.attach(11);
  servoVertical.attach(10);
  servoVertical.write(0);
  servoHorisontal.write(0);
} 
 
void loop() { 
 
  photoSensorA0 = analogRead(A0);
  photoSensorA1 = analogRead(A1);
  photoSensorA2 = analogRead(A2);
  photoSensorA3 = analogRead(A3);
  
  
  
  
  }  
